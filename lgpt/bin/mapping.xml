<!--
    MAPPING FOR KEYBOARDS
    belongs in location of binary
    -=-=These keys might not match your controller!=-=-
    enable DUMPEVENT to help with mapping
-->
<MAPPINGS>

	<MAP src="key:0:c" dst="/event/up" />
	<MAP src="key:0:d" dst="/event/down" />
	<MAP src="key:0:e" dst="/event/left" />
	<MAP src="key:0:f" dst="/event/right" />

	<MAP src="key:0:g" dst="/event/a" />
	<MAP src="key:0:j" dst="/event/b" />

	<MAP src="key:0:m" dst="/event/rshoulder" />
	<MAP src="key:0:k" dst="/event/lshoulder" />
	<MAP src="key:0:o" dst="/event/start" />
	<MAP src="key:0:n" dst="/tempo/tap" /> 


</MAPPINGS>
